<?xml version="1.0" encoding="UTF-8" ?><odoo>    <menuitem id="menu_centros" name="Centros" sequence="10" />    <record id="school_list_view" model="ir.ui.view">        <field name="name">school.list.view</field>        <field name="model">school.profile</field>        <field name="arch" type="xml">            <tree>                <field name="name" string="From List Changed."/>                <field name="email" />                <field name="phone" />            </tree>        </field>    </record>    <record id="school_form_view" model="ir.ui.view">        <field name="name">school.form.tree</field>        <field name="model">school.profile</field>        <field name="arch" type="xml">            <form>                <group>                    <group>                        <field name="name" />                        <field name="email"                               placeholder="abc@gmail.com"/>                        <field name="phone" />                        <field name="is_virtual_class"/>                        <field name="school_rank" />                        <field name="result"/>                        <field name="address"/>                        <field name="auto_rank" nolabel="1" />                    </group>                    <group>                        <field name="estalish_date" />                        <field name="open_date"/>                        <field name="school_type" />                    </group>                </group>                <group>                    <field name="documents" filename="document_name"                           />                    <field name="document_name" invisible="1"/>                    <field name="school_image" widget="image" invisible="1"/>                    <field name="school_description"                           string="School Description"/>                </group>                <group>                </group>            </form>        </field>    </record>    <record id="action_school_profile" model="ir.actions.act_window">        <field name="name">School Profile</field>        <field name="res_model">centro.colegio</field>        <field name="view_mode">tree,form</field>    </record>    <menuitem id="school_root_menu" name="School" />    <menuitem id="school_sub_menu" parent="school_root_menu"              name="School Profile"/>    <menuitem id="school_profile_menu" name="School"              action="action_school_profile"              parent="school_sub_menu"/></odoo>